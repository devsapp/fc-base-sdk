{
  "name": "fc-base-sdk",
  "version": "0.0.20",
  "main": "./dist/index.js",
  "dependencies": {
    "@alicloud/fc2": "^2.2.2",
    "@alicloud/pop-core": "^1.7.10",
    "@serverless-devs/core": "^0.1.15-beta.2",
    "lodash": "^4.17.20",
    "tty-table": "^4.1.3"
  },
  "autoInstall": false,
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.171",
    "@types/node": "14",
    "esbuild": "^0.14.0",
    "f2elint": "^0.4.4",
    "jest": "^26.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.14",
    "ts-node": "^9.1.1",
    "typescript": "^3.9.7"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "npx tsc -w -p tsconfig.json",
    "prebuild": "npm i rimraf --no-save && rimraf package-lock.json && rimraf dist && rimraf node_modules && npm i",
    "build": "npm run fix && npm run lint && npm run esbuild",
    "esbuild": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --outfile=dist/index.js",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
